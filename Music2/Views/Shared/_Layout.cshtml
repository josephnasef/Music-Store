<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <link rel="shortcut icon" type="image/x-icon" href="https://i.pinimg.com/originals/fd/50/42/fd5042a1f17c0b1340a3a4d35f503b98.png">

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />

</head>
<body>

    <nav class="navbar navbar-inverse ">
        <div class="container-fluid">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand transtion" href="@Url.Content("~")">
                    MUSIC
                    STORE
                </a>
            </div>
            <ul id="navlist">
                <li class="first transtion">
                    <a href="@Url.Content("~")" id="current" class="transtion">
                        Home
                    </a>
                </li>
                <li><a class="transtion" href="@Url.Content("~/Store/")">Store</a></li>
                <li>@{Html.RenderAction("CartSummary", "ShoppingCart");}</li>
                <li>
                    <a href="@Url.Content("~/StoreManager/")" class="transtion">
                        Admin
                    </a>
                </li>
                @Html.Partial("_LoginPartial")
            </ul>


            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">


            </div><!-- /.navbar-collapse -->
        </div><!-- /.container-fluid -->
    </nav>

    @*<a class="btn btn-success " data-toggle="modal" data-target=".bootstrpmodal" href="#">content</a>*@
    <div class="modal fade bootstrpmodal ">
        
    </div>

    <div class="container">
        <div class="row">
            <div id="main" class="col-lg-2">
                @{Html.RenderAction("GenreMenu", "Store");}

            </div>
            <div class="col-md-10">
                @RenderBody()
            </div>
        </div>
    </div>



    @*<script src="@Url.Content("~/Scripts/jquery-1.4.4.min.js")" type="text/javascript"></script>*@
    <script src="~/Scripts/jquery-3.4.1.min.js" type="text/javascript"></script>
    <script src="~/Scripts/bootstrap.min.js" type="text/javascript"></script>
    @*<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>*@
    <script src="~/Scripts/jquery-3.4.1.min.js " type="text/javascript"></script>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
</body>
</html>